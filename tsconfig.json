{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "jsx": "react-jsx",           // ✅ JSX 파서 추가
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": false,       // ✅ any 허용
      "strict": false,              // ✅ 엄격모드 해제
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*"]
  }
  